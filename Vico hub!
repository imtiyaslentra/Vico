	lp.Character.Humanoid.Jumping:Connect(function(state)

		if (state and thingsEnabled.JumpCircleEnabled) then

			EmiteJC(lp.Character.HumanoidRootPart.Position - Vector3.new(0, 3, 0));

		end

	end);

end

