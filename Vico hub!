local function findBall()
	local FlatIdent_95CAC = 0;
	local self;
	while true do
		if (FlatIdent_95CAC == 0) then
			self = balls:FindFirstChildOfClass("Part");
			return self;
		end
	end
end
local function ParryAttempt(args)
	remotesStorage:WaitForChild("ParryAttempt"):FireServer(unpack(args));
end
local function alive(plr)
	if (plr and (plr.Character ~= nil) and plr.Character:FindFirstChild("HumanoidRootPart") and plr.Character:FindFirstChild("Humanoid") and (plr.Character:FindFirstChild("Humanoid").Health > 0)) then
		return true;
	end
end
local function getPing()
	return game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValue();
end
local function say(text)
	rep:WaitForChild("DefaultChatSystemChatEvents"):WaitForChild("SayMessageRequest"):FireServer(text, "All");
end
local function openCase(caseName)
	local FlatIdent_76979 = 0;
	while true do
		if (FlatIdent_76979 == 1) then
			remotesStorage:WaitForChild("Store"):WaitForChild("GetOwnedAbilities"):InvokeServer();
			remotesStorage:WaitForChild("Store"):WaitForChild("GetOwnedSwordSkins"):InvokeServer();
			break;
		end
		if (FlatIdent_76979 == 0) then
			if (caseName == "Swords") then
				remotesStorage:WaitForChild("Store"):WaitForChild("RequestOpenSwordBox"):InvokeServer();
			elseif (caseName == "Explosions") then
				remotesStorage:WaitForChild("Store"):WaitForChild("RequestOpenExplosionBox"):InvokeServer();
			end
			remotesStorage:WaitForChild("Store"):WaitForChild("GetOwnedExplosionFXes"):InvokeServer();
			FlatIdent_76979 = 1;
		end
	end
end
function checkForInvisiblity(player)
	local FlatIdent_24A02 = 0;
	local char;
	local torso;
	local hlInChar;
	while true do
		if (FlatIdent_24A02 == 1) then
			hlInChar = char:FindFirstChildOfClass("Highlight");
			return torso and (torso.Transparency == 1) and hlInChar;
		end
		if (FlatIdent_24A02 == 0) then
			char = player.Character;
			torso = char:FindFirstChild("Torso");
			FlatIdent_24A02 = 1;
		end
	end
end
local function getPlayers()
	return players:GetPlayers();
end
