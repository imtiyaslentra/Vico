				if (FlatIdent_89ECE == 0) then
					oldColorTheme = ColorTheme;
					DesyncPart.Color = ColorTheme[1];
					FlatIdent_89ECE = 1;
				end
				if (FlatIdent_89ECE == 4) then
					getgenv().AmbientVFX.ParticleEmitter.Color = ColorSequence.new({ColorSequenceKeypoint.new(0, ColorTheme[1]),ColorSequenceKeypoint.new(1, ColorTheme[2])});
