												FlatIdent_5ED46 = 8;

											end

										end

									end

									break;

								end

							end

						end

					end);

				end

			else

				visualPart.Position = Vector3.new(0, 10000, 0);

				if (hLoop ~= nil) then

					local FlatIdent_4D434 = 0;

					while true do

						if (FlatIdent_4D434 == 0) then

							hLoop:Disconnect();

							hLoop = nil;

							break;

						end

					end

				end

			end

		end);

		coroutine.resume(func);

	end});

	combatCategory_main.CreateDropdown({name="Target",side="left",objects={"Random","Closest"},choosed="Closest",callback=function(state)

		getgenv().parryTarget = state;

